{
  "name": "rubik-pure-scaffold",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build-cdn": "cross-env CDN_ENV=true yarn run rubik build",
    "build-no-hash": "cross-env NO_HASH_ENV=true yarn run rubik build",
    "build-qa": "yarn run rubik build --mode qa",
    "build": "yarn run rubik build",
    "start": "yarn run rubik serve",
    "start-qa": "yarn run rubik serve --mode qa",
    "lint": "yarn run rubik lint",
    "commitlint": "cross-env DEBUG=common-bin yarn run rubik commit",
    "rubik": "node ../../bin/rubik.js"
  },
  "keywords": [
    "webpack",
    "multiple",
    "pages"
  ],
  "author": "Fancy <fancyboynet@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^5.2.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.1.0"
  },
  "dependencies": {
    "axios": "^0.19.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-merge": "rubik check-package",
      "post-checkout": "rubik check-package",
      "commit-msg": "rubik commit"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "yarn run rubik lint",
      "git add"
    ]
  }
}
